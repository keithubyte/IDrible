<com.nirhart.parallaxscroll.views.ParallaxListView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:nirhart="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/about_listview"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:headerDividersEnabled="true"
    android:footerDividersEnabled="true"
    android:scrollbarStyle="outsideOverlay"
    android:clipToPadding="false"
    android:scrollingCache="true"
    android:smoothScrollbar="true"
    android:dividerHeight="0dp"
    android:divider="@android:color/transparent"
    nirhart:parallax_factor="2.0"
    android:background="@color/dribbble_background"
    tools:listitem="@layout/item_about"
    tools:context="com.keith.idribbble.ui.AboutActivity">

</com.nirhart.parallaxscroll.views.ParallaxListView>
